<%- if @engine_match and @engine_listen -%>
    <FilesMatch <%= @engine_match %>>
        SetHandler proxy:<%= @engine_listen %>
    </FilesMatch>
<%- end -%>

<%- Array(@apps).each do |app| -%>
    <FilesMatch <%= app['match'] %>>
        SetHandler proxy:<%= app['listen'] %>
    </FilesMatch>
<%- end -%>

<%- if @custom_fragment -%>
    <%= @custom_fragment %>
<%- end -%>
