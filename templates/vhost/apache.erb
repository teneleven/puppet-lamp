<%- if @engine_match and @engine_handler -%>
    <FilesMatch <%= @engine_match %>>
        SetHandler <%= @engine_handler %>
    </FilesMatch>
<%- end -%>

<%- if @proxy and @proxy_match -%>
    ProxyPass "<%= @proxy_match %>" "<%= @proxy %>"
    ProxyPassReverse "<%= @proxy_match %>" "<%= @proxy %>"
<%- end -%>

<%- if @custom_fragment -%>
    <%= @custom_fragment %>
<%- end -%>
