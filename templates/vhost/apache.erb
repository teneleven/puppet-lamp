<%- if @engine_match and @engine_listen -%>
    ProxyPassMatch "<%= @engine_match %>" "<%= @engine_listen %><%= @path %>"
<%- end -%>

<%- @apps.each do |name,app| -%>
    <%- if app['listen'] -%>
        ProxyPassMatch "<%= app['match'] %>" "<%= app['listen'] %>"
    <%- elsif app['script'] -%>
        ProxyPassMatch "<%= app['match'] %>" "<%= @engine_listen %><%= app['script'] %>"
    <%- end -%>
<%- end -%>

<%- if @custom_fragment -%>
    <%= @custom_fragment %>
<%- end -%>
