<%- if @engine_match and @engine_listen -%>
    ProxyPassMatch "<%= @engine_match %>" "<%= @engine_listen %>"
<%- end -%>

<%- if @proxy and @proxy_match -%>
    ProxyPass "<%= @proxy_match %>" "<%= @proxy %>"
    ProxyPassReverse "<%= @proxy_match %>" "<%= @proxy %>"
<%- end -%>

<%- if @custom_fragment -%>
    <%= @custom_fragment %>
<%- end -%>
